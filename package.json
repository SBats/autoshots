{
  "name": "autoshots",
  "version": "1.1.0",
  "description": "Screenshots automation for web applications with PhantomJS",
  "main": "screenshots.js",
  "scripts": {
    "post-install": "npm run init",
    "init": "cp ./example.config.json ./config.json",
    "start": "npm run shots",
    "shots": "phantomjs --web-security=false screenshots.js config.json"
  },
  "dependencies": {
    "phantomjs-prebuilt": "^2.1.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SBats/autoshots.git"
  },
  "keywords": [
    "screenshots",
    "automation",
    "web-app"
  ],
  "author": "Sbats",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SBats/autoshots/issues"
  },
  "homepage": "https://github.com/SBats/autoshots#readme"
}
